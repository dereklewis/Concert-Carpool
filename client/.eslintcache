bj = {\n      //   eventName: \"Tame Impala\",\n      //   eventLocation: \"Red Rocks\",\n      //   eventDate: \"2021-10-07\",\n      //   driver: \"true\",\n      //   passenger: \"false\",\n      //   notes: \"yay\",\n      //   profile: \"6162068e82ea7fe7a5d35ce2\",\n      // };\n      const { data } = await addEvent({\n        variables: {\n          ...eventForm,\n        },\n      });\n      //need to redirect react-router here -> whatever we are showing in our cards\n      console.log(\"this is the data\", JSON.stringify(data));\n    } catch (error) {\n      console.log(\"this is an event error\", error);\n    }\n  };\n\n  return (\n    <div>\n      {Auth.loggedIn() ? (\n        <Form onSubmit={handleFormSubmit}>\n          <Form.Group className=\"mb-3\" controlId=\"formBasicEmail\">\n            <Form.Label>Event Name</Form.Label>\n            <Form.Control\n              type=\"text\"\n              placeholder=\"Enter event name\"\n              name=\"eventName\"\n              value={eventForm.eventName}\n              onChange={(event) => handleInputChange(event)}\n            />\n          </Form.Group>\n          <Form.Group className=\"mb-3\" controlId=\"formBasicEmail\">\n            <Form.Label>Event Location</Form.Label>\n            <Form.Control\n              type=\"text\"\n              placeholder=\"Enter event location\"\n              name=\"eventLocation\"\n              value={eventForm.eventLocation}\n              onChange={(event) => handleInputChange(event)}\n            />\n          </Form.Group>\n          <Form.Group className=\"mb-3\" controlId=\"formBasicEmail\">\n            <Form.Label>Event Date</Form.Label>\n            <Form.Control\n              type=\"text\"\n              placeholder=\"Enter the event date\"\n              name=\"eventDate\"\n              value={eventForm.eventDate}\n              onChange={(event) => handleInputChange(event)}\n            />\n          </Form.Group>\n\n          <Form.Group className=\"mb-3\" controlId=\"formBasicPassword\">\n            <Form.Label>Driver (true/false)</Form.Label>\n            <Form.Control\n              type=\"text\"\n              placeholder=\"Would you like to be a driver\"\n              name=\"driver\"\n              value={eventForm.driver}\n              onChange={(event) => handleInputChange(event)}\n            />\n          </Form.Group>\n          <Form.Group className=\"mb-3\" controlId=\"formBasicPassword\">\n            <Form.Label>Passenger(true/false)</Form.Label>\n            <Form.Control\n              type=\"text\"\n              placeholder=\"Would you like to be a passenger\"\n              name=\"passenger\"\n              value={eventForm.passenger}\n              onChange={(event) => handleInputChange(event)}\n            />\n          </Form.Group>\n          <Form.Group className=\"mb-3\" controlId=\"exampleForm.ControlTextarea1\">\n            <Form.Label>\n              Notes - add carpool notes here! Ex: Time of arrival to the venue\n            </Form.Label>\n            <Form.Control\n              as=\"textarea\"\n              rows={3}\n              name=\"notes\"\n              value={eventForm.notes}\n              onChange={(event) => handleInputChange(event)}\n            />\n          </Form.Group>\n          <Button variant=\"primary\" type=\"submit\" href=\"/events\">\n            Submit\n          </Button>\n        </Form>\n      ) : (\n        <p>\n          You need to be logged in to fill out an event form. Please{\" \"}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default EventForm;\n","/Users/dwreck/Desktop/Projects/Concert-Carpool/client/src/components/Header/Header.js",[],"/Users/dwreck/Desktop/Projects/Concert-Carpool/client/src/utils/mutations.js",[],"/Users/dwreck/Desktop/Projects/Concert-Carpool/client/src/utils/auth.js",[],"/Users/dwreck/Desktop/Projects/Concert-Carpool/client/