assName=\"mb-3\" controlId=\"formBasicEmail\">\n            <Form.Label>Event Date</Form.Label>\n            <Form.Control\n              type=\"email\"\n              placeholder=\"Enter the event date\"\n              value={event}\n              onChange={(event) => setEvent(event.target.value)}\n            />\n          </Form.Group>\n\n          <Form.Group className=\"mb-3\" controlId=\"formBasicPassword\">\n            <Form.Label>Driver</Form.Label>\n            <Form.Control\n              type=\"password\"\n              placeholder=\"Would you like to be a driver\"\n              value={event}\n              onChange={(event) => setEvent(event.target.value)}\n            />\n          </Form.Group>\n          <Form.Group className=\"mb-3\" controlId=\"formBasicPassword\">\n            <Form.Label>Passenger</Form.Label>\n            <Form.Control\n              type=\"password\"\n              placeholder=\"Would you like to be a passenger\"\n              value={event}\n              onChange={(event) => setEvent(event.target.value)}\n            />\n          </Form.Group>\n          <Form.Group className=\"mb-3\" controlId=\"exampleForm.ControlTextarea1\">\n            <Form.Label>\n              Notes - add carpool notes here! Ex: Time of arrival to the venue\n            </Form.Label>\n            <Form.Control\n              as=\"textarea\"\n              rows={3}\n              value={event}\n              onChange={(event) => setEvent(event.target.value)}\n            />\n          </Form.Group>\n          <Button variant=\"primary\" type=\"submit\">\n            Submit\n          </Button>\n        </Form>\n      ) : (\n        <p>\n          You need to be logged in to fill out an event form. Please{\" \"}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default EventForm;\n","/Users/dwreck/Desktop/Projects/Concert-Carpool/client/src/utils/auth.js",[],"/Use